					// Data Element mouseover
					if ((elementDetails.element !== Homr.DUP_STRING) && (typeof elementDetails.element !== 'undefined')) {
						elementTableHtml += "<div class='phr_details'>" +
							"<div class='phr_details_title'>" + 
