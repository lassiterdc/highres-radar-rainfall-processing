        } else if ($("#PHR_TAB").hasClass("homrCurrentTab") === true) {
          linktab = 'PHR';
          $("#print_link").hide();
          $("#save_link").hide();
          $("#homrMshrContainer").hide();
          $("#homrLocationsContainer").hide();
          $("#homrMiscContainer").hide();
