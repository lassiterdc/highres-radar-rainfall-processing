$(document).ready( 
  function () {
    $(".file").click(
      function () {
        if (typeof ga !== 'undefined') {
          try {
            // strip the jsession id from url's 
            var re = /;jsessionid=\w+$/;
            var file = $(this).attr('href').replace(re, '');
						ga('send', 'event', 'HOMR', 'FileDownload', file);
          } catch (error) {

          }
        }
      }
    );
  }
);